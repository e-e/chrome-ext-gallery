{
  "short_name": "tameyo",
  "name": "tameyo",
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "48": "images/icon.png"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "tameyo"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}